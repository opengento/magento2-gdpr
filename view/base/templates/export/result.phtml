/** @var \Magento\Framework\View\Element\Template $block */

/** @var \Magento\Framework\View\Element\RendererList $rendererList */
$rendererList = $block->getChildBlock('exportRenderer');

/** @var \Magento\Framework\DataObject $viewModel */
$viewModel = $block->getData('viewModel');
?>
<?php foreach ($viewModel->getData() as $key => $value): ?>
    <?=
        $rendererList->getRenderer($key, 'default', 'Opengento_Gdpr::export/renderer/default.phtml')
                     ->setData('viewModel', new \Magento\Framework\DataObject([$key => $value]))
                     ->toHtml()
    ?>
<?php endforeach; ?>
